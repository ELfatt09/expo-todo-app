{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "moduleResolution": "node",
    "skipLibCheck": true,

    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"]
    }
  }
}
